// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BattlegroundsCardNft {
  tokenId String @id
  name String
  minionType String
  baseAttackStat Int
  baseHealthStat Int
  tavernTier Int
}
